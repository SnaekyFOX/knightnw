command /viphız:
        permission: vip.hız
        permission message: &6&lVIP &8► &cBunu kullanmak için yetkin yok!
        trigger:
                set {_difference} to difference between {vipozellik.%player%} and now
                if {_difference} is less than 5 minutes:
                        set  {_difference} to difference between {_difference} and 1 minutes
                        message "&6&lVIP &8► &cLütfen önceki efektin bitmesini bekle!"
                        stop
                set {vipozellik.%player%} to now
                apply potion of speed 2 to the player for 60 seconds
                message "&6&lVIP &8► &7Hız &7efekti verildi."


