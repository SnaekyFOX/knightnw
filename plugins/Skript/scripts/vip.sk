command /vipver <player>:
	permission: console.admin
	trigger:
		execute console command "lp user %arg-1% parent clear"
		execute console command "lp user %arg-1% parent addtemp vip 30d"
		execute console command "lp user %arg-1% parent switchprimarygroup vip"
		send players title "&6&l%arg-1%" with subtitle "&aVIP Satın Aldı!"
		set action bar of players to "&6&l%arg-1% &8» &aVIP satın aldı!"
		send "&6&lMağaza &8» &b%arg-1% &6VIP &ASatın Aldı!" to players
		
