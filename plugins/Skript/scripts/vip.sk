command /viphediye:
    permission: vip.hediye
    permission message: Yetkin yok
    trigger:
        {viphediyealdi.%player%} isn't set:
            set {viphediyealdi.%player%} to true
            execute console command "hsp give endtasi %player% 3"
            execute console command "hsp give denizfeneri %player% 1"
            execute console command "hsp give altincevheri %player% 4"
            execute console command "hsp give demircevheri %player% 8"
            execute console command "crazycrates give p para 4 %player%"
            execute console command "crazycrates give p vip 1 %player%"
            execute console command "crazycrates give p spawner 4 %player%"
            execute console command "crazycrates give p event 4 %player%"
            execute console command "eco give %player% 1000000000" 
            send "&6&lVipHediye &8► &eHediyenizi başarıyla aldınız."
            wait 30 days
            set {viphediyealdi.%player%} to false
        else:
            send "&6&lVipHediye &8► &cAyda bir defa alabilirsin."
