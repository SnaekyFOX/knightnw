command /komurcevherisp:
    trigger:
        if player's balance is bigger than 50000000:
            message "&6Spawner&fMarket &8► &aKömür cevheri spawner satın aldınız. &7(&81x&7)"
            remove 50000000 from player's balance
            execute console command "hsp give komurcevheri %player% 1"
        else:
            message "&6Spawner&fMarket &8► &cYeterli para miktarınız yok!"
			
