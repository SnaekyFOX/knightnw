command /fpver <player>:
	permission: console.admin
	permission message: &cCoin kasasını açarsan bu komutu uygular sana kasmayana ekmek yokkk!!!
	trigger:
		execute console command "lp user %arg-1% permission set fastplace.use" 
		execute console command "lp user %arg-1% permission set fastplace.level.1" 
		stop
