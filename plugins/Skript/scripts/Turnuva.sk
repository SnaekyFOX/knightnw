command /teklekırmızı <player> <text>:
    permission: turnuva.admin
    permission message: &6&lTurnuva&8» &cMaalesef, Bunu yapamazsın!
    usage: /turnuvabilgi
    trigger:
        arg-1 is set:
            send "&6&lTurnuva&8» &a%arg-1% oyuncusunu kırmızı takıma eklediniz. &8(&2&l%arg-2%&8)" to player
            send "&6&lTurnuva&8» &aTurnuva yetkilisi seni kırmızı takıma ekledi! &8(&2&l%arg-2%&8)" to arg-1
            wait 1 second
            send "&6&lTurnuva&8» &aSeni turnuva alanına ışınlıyorum. Envanterin silinecek!" to arg-1
            wait 8 second
            clear arg-1's inventory
            heal arg-1
            teleport arg-1 to {turnuva.kırmızı}
            wait 1 seconds
            execute console command "turnuvaitem %arg-1%"
            send "&6&lTurnuva&8» &aTurnuva alanına başarıyla ışınlandınız." to arg-1
        arg-1 is not set:
            send "&6&lTurnuva&8» &cLütfen oyuncu ismi giriniz." to player
             
