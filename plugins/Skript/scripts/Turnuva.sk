command /tsetkırmızı:
    permission: turnuva.admin
    permission message: &6&lTurnuva&8» &cMaalesef, Bunu yapamazsın!
    usage: /turnuvabilgi
    trigger:
        set {turnuva.kırmızı} to location of player
        send "&6&lTurnuva&8» &aBaşarıyla kırmızı takım alanı oluşturuldu" to player
        stop
        
command /tsetmavi:
    permission: turnuva.admin
    permission message: &6&lTurnuva&8» &cMaalesef, Bunu yapamazsın!
    usage: /turnuvabilgi
    trigger:
        set {turnuva.mavi} to location of player
        send "&6&lTurnuva&8» &aBaşarıyla mavi takım alanı oluşturuldu." to player
