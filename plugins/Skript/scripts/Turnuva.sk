command /tkick <player>:
    permission: turnuva.admin
    permission message: &6&lTurnuva&8» &cMaalesef, Bunu yapamazsın!
    usage: /turnuvabilgi
    trigger:
        arg-1 is set:
            send "&6&lTurnuva&8» &a%arg-1% öldüğü için turnuvadan çıkarıldı." 
            send "&6&lTurnuva&8» &cYetkili seni öldüğün için turnuvadan çıkardı!" to arg-1
            heal arg-1
            clear arg-1's inventory
            teleport arg-1 to {turnuva.kick}
            stop
            
command /treload:
    permission: turnuva.admin
    permission message: &cMaalesef, Bunu yapamazsın!
    usage: /turnuvabilgi
    trigger:
        execute console command "sk reload turnuva"
        wait 1 second
        send "&6&lTurnuva&8» &aTurnuva skripti başarıyla yeniden yüklendi." 
     
command /turnuvabilgi:
    permission: turnuva.admin
    permission message: &cMaalesef, Bunu yapamazsın!
    trigger:
        send ""
        send "      &6&lTURNUVA MENU" to player
        send "&8" to player
        send "&8 » &6/tsetkırmızı &8| &eKırmızı takımın spawn noktasını ayarlar." to player
        send "&8 » &6/tsetmavi &8| &eMavi takımın spawn noktasını ayarlar." to player
        send "&8 » &6/tsetkick &8| &eKick spawn noktasını ayarlar." to player
        send "&8 » &6/teklemavi (oyuncu) (klan) &8| &eMavi takıma eklenecek oyuncuyu ve klanı bildirir." to player
        send "&8 » &6/teklekırmızı (oyuncu) (klan) &8| &eKırmızı takıma eklenecek oyuncuyu ve klanı bildirir." to player
        send "&8 » &6/tkick (oyuncu) &8| &eOyuncuyu turnuvadan çıkarır." to player
        send "&8 » &6/treload &8| &eTurnuva skriptini yeniden başlatır." to player
        send "&8" to player
        
command /turnuvaitem <player>:
    permission: turnuva.admin
    permission message: &cMaalesef, Bunu yapamazsın!
    usage: /turnuvaitem oyuncu
    trigger:
        give 1 276 of sharpness 6 and smite 5 and unbreaking 3 named "&6&lTurnuva Kılıç" to arg-1
        give 3 322:1 named "&6&lTurnuva Gapple" to arg-1
        give 2 373:8233 named "&6&lGüç İksiri" to arg-1
        give 2 373:8226 named "&6&lHız İksiri" to arg-1
        equip arg-1 with diamond chestplate of protection 6 and unbreaking 3 named "&6&lTurnuva Zırh"
        equip arg-1 with diamond helmet of protection 6 and unbreaking 3 named "&6&lTurnuva Zırh"
        equip arg-1 with diamond leggings of protection 6 and unbreaking 3 named "&6&lTurnuva Zırh"
        equip arg-1 with diamond boots of protection 6 and unbreaking 3 named "&6&lTurnuva Zırh"
		
