command /tamir <text>:
	usage: /tamir <el/hepsi>
	trigger:
		if arg-1 is set:
			if arg-1 is "el" or "hand":
				if player's held item is tool:
					if player's balance is bigger than 1000000000:
						repair tool of player
						remove 1000000000 from player's balance
						send "&3Tamirci &8>> &aBaşarıyla elindeki eşyayı tamir ettin."
					else:
						send "&3Tamirci &8>> &cYeterli parayı karşılayamıyorsunuz."
			if arg-1 is "all" or "hepsi":
				if player's balance is bigger than 4000000000:
					execute console command "lp user %player% permission set essentials.repair"
					execute console command "lp user %player% permission set essentials.repair.enchanted"
					execute console command "lp user %player% permission set essentials.repair.armor"
					execute console command "lp user %player% permission set essentials.repair.all"
					execute player command "repair all"
					execute console command "lp user %player% permission unset essentials.repair"
					execute console command "lp user %player% permission unset essentials.repair.enchanted"
					execute console command "lp user %player% permission unset essentials.repair.armor"
					execute console command "lp user %player% permission unset essentials.repair.all"
					remove 4000000000 from player's balance
					send "&3Tamirci &8>> &aBaşarıyla envanterindeki eşyaları tamir ettin."
				else:
					send "&3Tamirci &8>> &cYeterli parayı karşılayamıyorsunuz."
					stop

