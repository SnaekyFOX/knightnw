command /komut [<text>]:
	trigger:
		if arg 1 is set:
			if arg 1 is "aç":
				player has permission "sohbet.admin":
					set {komut::%player%} to true
					send "&6&lSKYBLOCK &8► &aArtık oyuncuların kullandıkları komutları görebilirsin."
				else:
					send ""
			if arg 1 is "kapat":
				player has permission "sohbet.admin":
					set {komut::%player%} to false
					send "&6&lSKYBLOCK &8► &cArtık oyuncuların kullandıkları komutları göremezsin."
				else:
					send ""
		else:
			send " &e/komut &c<aç>&7, &c<kapat>"
		
on chat:
    if player doesn't have permission "allow.spam":
        set {_difference} to difference between {chatspam::%player%} and now
        if {_difference} is less than 2 seconds:
            set  {_difference} to difference between {_difference} and 2 seconds
            cancel event
            send "&6&lSkyBlock &8► &7Lütfen tekrar yazabilmek için &a3 &7saniye bekleyiniz." to player
        set {chatspam::%player%} to now
		
on join:
    delete {chatspam::%player%}
    
on quit:
    delete {chatspam::%player%}
    
on script load:
    delete {chatspam}

command /sohbet <text>:
	permission: sohbet.kilit
	permission message: &6&lSkyBlock &8► &cBu komutu girmek için yetkiniz yok.
	trigger:
		if arg 1 is "kapat":
			set {kilitli} to true
			send "&6&lSkyBlock &8► &cSohbet &4%player% &ctarafından kapatıldı." to players
		else:
			if arg 1 is "aç":
				set {kilitli} to false
				send "&6&lSkyBlock &8► &aSohbet &b%player% &atarafından açıldı." to players			
on chat:
	{kilitli} is true:
		if player doesn't have permission "sohbet.bypass":
			cancel event
			send "&6&lSkyBlock &8► &cSohbet şuanda kapalıdır, hata olduğunu düşünüyorsan yetkiliye ulaşabilirsin." to player
		else:
			uncancel event
			
