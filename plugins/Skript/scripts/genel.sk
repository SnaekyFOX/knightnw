command /setspawn:
	permission: admin.spawn
	permission message: &cAh be koçum olmaz bu iş böyle ama :(
	trigger:
		set {spawn} to location of player
		send "&aSpawn ayarlandı."
		stop
		
command /spawn:
	trigger:
		teleport player to {spawn}

on first join:
	wait 1 seconds
	teleport player to {spawn}
	loop 200 times:
		send ""
		
on first join:
   execute console command "lp user %player% parent set oyuncu"
   teleport player to {spawn}
	
on join:
	teleport player to {spawn}
	set join message to "&7[&2+&7] &a%player%"
	loop 200 times:
		send ""
	
on quit:
	set quit message to "&7[&4-&7] &c%player%"
		
command /sil:
	permission: sil.admin
	permission message: &cYetkiniz yok.
	trigger:
		loop 200 times:
			send "" to players
