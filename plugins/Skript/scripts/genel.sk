command /adareklam:
    trigger:
        set {_beklemesurresi} to difference between {adareklamsure::%player%} and now
        if {_beklemesurresi} is less than 5 minutes:
            set {_beklemesurresi} to difference between {_beklemesurresi} and 1 minutes
            set {_beklemesurresi} to "%{_beklemesurresi}%"
            replace all "minutes" with "dakika" in {_beklemesurresi}
            replace all "minute" with "dakika" in {_beklemesurresi}
            replace all "seconds" with "saniye" in {_beklemesurresi}
            replace all "second" with "saniye" in {_beklemesurresi}
            replace all " and " with " " in {_beklemesurresi}
            send "&6&lSKYBLOCK &8&l► &eAda Reklam için &d%{_beklemesurresi}% &ebeklemelisin."
        else:
            player's balance is greater than 10000000000:
                remove 10000000000 from player's balance
                set {adareklamsure::%player%} to now
                send "&8&l&m----------------[&6&lAda Duyuru&8&l&m]----------------" to players
                send "" to players
                send " &b➜ &d%player% &eadlı oyuncu sizi adasına davet ediyor" to players
                send " &b➜ &egitmek için &d/is warp %player%" to players
                send "" to players
                send "&8&l&m----------------[&6&lAda Duyuru&8&l&m]----------------" to players
            else:
                send "&6&lSKYBLOCK &8&l► &eBakiyen yetersiz &d10 milyar &eparan olması lazım!"
				
